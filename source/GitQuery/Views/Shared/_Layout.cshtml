                <div class="text-center">
                    GitQuery - @DateTime.Now.Year
                </div>
            </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <!-- page -->
    <!-- Scripts -->
    <script type="text/javascript" src="~/Mist/light/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Mist/light/js/bootstrap.min.js"></script>
