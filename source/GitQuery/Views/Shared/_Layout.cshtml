    <script type="text/javascript" src="~/Mist/light/js/jquery.countTo.js"></script>
    <!-- Twitter Feed -->
    <script type="text/javascript" src="~/Mist/light/js/tweet/carousel.js"></script>
    <script type="text/javascript" src="~/Mist/light/js/tweet/scripts.js"></script>
    <script type="text/javascript" src="~/Mist/light/js/tweet/tweetie.min.js"></script>
    <!-- Background Video -->
    <script type="text/javascript" src="~/Mist/light/js/jquery.mb.YTPlayer.js"></script>
    <!-- Fancy select -->
    <script type="text/javascript" src="~/Mist/light/js/fancySelect.js"></script>
    <!-- Custom Js Code -->
