    <script type="text/javascript" src="~/Mist/light/js/jquery.easing.1.3.js"></script>
    <!-- Sticky Menu -->
    <script type="text/javascript" src="~/Mist/light/js/jquery.sticky.js"></script>
    <!-- Bootstrap Validation -->
    <script type="text/javascript" src="~/Mist/light/js/bootstrapValidator.min.js"></script>
    <!-- Revolution Slider -->
    <script type="text/javascript" src="~/Mist/light/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript" src="~/Mist/light/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="~/Mist/light/js/revolution-custom.js"></script>
    <!-- Portfolio Filter -->
    <script type="text/javascript" src="~/Mist/light/js/jquery.mixitup.min.js"></script>
    <!-- Animations -->
    <script type="text/javascript" src="~/Mist/light/js/jquery.appear.js"></script>
    <script type="text/javascript" src="~/Mist/light/js/effect.js"></script>
